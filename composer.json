{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "composer-runtime-api": "^2.2",
        "amphp/http-client": "^4.6",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3.1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "egulias/email-validator": "^2.1.14",
        "elao/enum": "^2.0",
        "fakerphp/faker": "^1.20",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "helios-ag/fm-elfinder-bundle": "^12.1",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "knplabs/knp-paginator-bundle": "^5.9",
        "liip/imagine-bundle": "^2.10",
        "monolog/monolog": "^3.3",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.8",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.2.*",
        "symfony/cache": "6.2.*",
        "symfony/config": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/lock": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mime": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.2.*",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/rate-limiter": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/web-link": "6.2.*",
        "symfony/yaml": "6.2.*",
        "symfonycasts/verify-email-bundle": "^1.13",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.21",
        "victor-prdh/recaptcha-bundle": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "DataFixtures\\": "src/DataFixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "prepare": [
            "symfony console doctrine:database:drop -f",
            "symfony console doctrine:database:create",
            "symfony console make:migration",
            "symfony console doctrine:migrations:migrate -n",
            "symfony console doctrine:fixtures:load --no-interaction",
            "php bin/console cache:clear --env=dev"
        ],

        "test": [
            "php bin/console doctrine:database:drop --force --env=test",
            "php bin/console doctrine:database:create --env=test",
            "php bin/console doctrine:schema:update --complete --force --env=test",
            "php bin/console doctrine:fixtures:load --no-interaction --env=test",
            "php bin/console cache:clear --env=test",
            "php bin/phpunit"
        ],
        "coverage": [
            "php bin/console doctrine:database:drop --force --env=test",
            "php bin/console doctrine:database:create --env=test",
            "php bin/console doctrine:schema:update --complete --force --env=test",
            "php bin/console doctrine:fixtures:load --no-interaction --env=test",
            "php bin/console cache:clear --env=test",
            "php bin/phpunit --coverage-html public/coverage"
        ],
        "phpstan": [
            "phpstan analyse src --level 8",
            "phpcs --standard=PSR12 src",
            "phpcs --standard=PSR12 tests"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "6.2.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "6.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/debug-bundle": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    }
}
